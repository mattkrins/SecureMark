{
	"version": "1.0",
	"name": "SecureMark",
	"author": "Matt Krins",
	"homepage_url": "https://github.com/mattkrins/",
	"description": "A bookmark encryption tool.",
	"permissions": [ "bookmarks" ],
	"options_page": "options.html",
	"browser_action": {
	  "default_title": "SecureMark",
	  "default_icon": "img/icon.png",
	  "default_popup": "popup.html"
	},
	"icons": {
		"16": "img/16.png",
		"48": "img/48.png",
		"128": "img/128.png"
	},
	"manifest_version": 2
}
